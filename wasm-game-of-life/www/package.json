  "dependencies": {
    "wasm-game-of-life": "link:../pkg"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.11.0",
    "hello-wasm-pack": "^0.1.0",
    "jest": "^27.5.1",
